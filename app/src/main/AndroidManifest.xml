<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sundram.urbanclapclone">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".framelayout.FrameGuideUrbanClap"></activity>
        <activity android:name=".framelayout.FramePayingServe" />
        <activity android:name=".framelayout.FrameBookingServe" />
        <activity android:name=".About" />
        <activity android:name=".CustomerSupport" />
        <activity android:name=".MyBookingActivity" />
        <activity android:name=".HomeScreenActivity" />
        <activity android:name=".DashBoard" />
        <activity
            android:name=".LoginActivity"
            android:parentActivityName=".OtpGeneratorActivity" />
        <activity
            android:name=".AboutUs"
            android:parentActivityName=".About" />
        <activity
            android:name=".OtpGeneratorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme"
            android:windowSoftInputMode="adjustNothing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>